<p id="notice"><%= notice %></p>

<h1>Listing Medical Team Medical Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Medical team</th>
      <th>Medical staff</th>
      <th>Speciality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medical_team_medical_staffs.each do |medical_team_medical_staff| %>
      <tr>
        <td><%= medical_team_medical_staff.medical_team %></td>
        <td><%= medical_team_medical_staff.medical_staff %></td>
        <td><%= medical_team_medical_staff.speciality %></td>
        <td><%= link_to 'Show', medical_team_medical_staff %></td>
        <td><%= link_to 'Edit', edit_medical_team_medical_staff_path(medical_team_medical_staff) %></td>
        <td><%= link_to 'Destroy', medical_team_medical_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

