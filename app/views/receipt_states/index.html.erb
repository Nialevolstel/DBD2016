<p id="notice"><%= notice %></p>

<h1>Listing Receipt States</h1>

<table>
  <thead>
    <tr>
      <th>Nombre stado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @receipt_states.each do |receipt_state| %>
      <tr>
        <td><%= receipt_state.nombre_stado %></td>
        <td><%= link_to 'Show', receipt_state %></td>
        <td><%= link_to 'Edit', edit_receipt_state_path(receipt_state) %></td>
        <td><%= link_to 'Destroy', receipt_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Receipt state', new_receipt_state_path %>
