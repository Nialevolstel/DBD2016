<p id="notice"><%= notice %></p>

<h1>Listing Medical Staffs</h1>

<table>
  <thead>
    <tr>
      <th>Rut personal medico</th>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Apellido paterno</th>
      <th>Apellido materno</th>
      <th>Sexo</th>
      <th>Telefono fijo</th>
      <th>Telefono movil</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medical_staffs.each do |medical_staff| %>
      <tr>
        <td><%= medical_staff.rut_personal_medico %></td>
        <td><%= medical_staff.primer_nombre %></td>
        <td><%= medical_staff.segundo_nombre %></td>
        <td><%= medical_staff.apellido_paterno %></td>
        <td><%= medical_staff.apellido_materno %></td>
        <td><%= medical_staff.sexo %></td>
        <td><%= medical_staff.telefono_fijo %></td>
        <td><%= medical_staff.telefono_movil %></td>
        <td><%= link_to 'Show', medical_staff %></td>
        <td><%= link_to 'Edit', edit_medical_staff_path(medical_staff) %></td>
        <td><%= link_to 'Destroy', medical_staff, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medical staff', new_medical_staff_path %>
