<p id="notice"><%= notice %></p>

<h1>Listing Article Clasifications</h1>

<table>
  <thead>
    <tr>
      <th>Nombre clasificacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @article_clasifications.each do |article_clasification| %>
      <tr>
        <td><%= article_clasification.nombre_clasificacion %></td>
        <td><%= link_to 'Show', article_clasification %></td>
        <td><%= link_to 'Edit', edit_article_clasification_path(article_clasification) %></td>
        <td><%= link_to 'Destroy', article_clasification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Article clasification', new_article_clasification_path %>
